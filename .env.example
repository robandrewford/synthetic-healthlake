# Health Platform Environment Variables
# Copy this file to .env and fill in your credentials
#
# Naming Convention: {PROJECT}_{SERVICE}_{OBJECT}
# See: docs/development/env-variable-naming-convention.md

# ==============================================================================
# SNOWFLAKE (HP_SNF_*)
# ==============================================================================

# Account locator (e.g., xy12345.us-east-1)
HP_SNF_ACCT=your_account_locator

# Service account credentials
HP_SNF_USER=your_username
HP_SNF_PASS=your_password

# Role to assume (ACCOUNTADMIN, ANALYST, etc.)
HP_SNF_ROLE=ACCOUNTADMIN

# Compute resources
HP_SNF_WH=COMPUTE_WH
HP_SNF_DB=HEALTH_PLATFORM_DB
HP_SNF_SCHEMA=RAW

# ==============================================================================
# AWS (HP_AWS_*) - Optional, uses default profile if not set
# ==============================================================================

# Named profile from ~/.aws/credentials
# HP_AWS_PROFILE=health-platform

# Default region for AWS operations
# HP_AWS_REGION=us-east-1

# ==============================================================================
# DBT (HP_DBT_*) - Optional overrides
# ==============================================================================

# Target environment (dev, staging, prod)
# HP_DBT_TARGET=dev

# Number of threads for parallel execution
# HP_DBT_THREADS=4
